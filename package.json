{
	"name": "svelte-datocms",
	"version": "0.6.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/lukaszwisniewski88/svelte-datocms"
	},
	"keywords": [
		"svelte",
		"datocms",
		"structuredtext",
		"blurhash"
	],
	"bugs": {
		"url": "https://github.com/lukaszwisniewski88/svelte-datocms/issues"
	},
	"license": "MIT",
	"scripts": {
		"dev": "svelte-kit dev",
		"package": "svelte-kit package",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "jest",
		"test:watch": "npm run test -- --watch"
	},
	"dependencies": {
		"blurhash": "^1.1.4",
		"datocms-listen": "^0.1.6",
		"datocms-structured-text-to-html-string": "^1.2.0",
		"datocms-structured-text-utils": "^1.2.0"
	},
	"devDependencies": {
		"@sveltejs/kit": "^1.0.0-next.191",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/svelte": "^3.0.3",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"cssnano": "^5.0.6",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"jest": "^27.3.1",
		"jsdom-testing-mocks": "^1.1.1",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.7",
		"svelte-jester": "^2.1.5",
		"svelte-preprocess": "^4.9.8",
		"svelte-use-form": "^2.4.2",
		"svelte2tsx": "^0.4.7",
		"ts-jest": "^27.0.7",
		"tslib": "^2.0.0",
		"typescript": "^4.4.4",
		"vite-plugin-windicss": "^1.4.12",
		"windicss": "^3.2.0"
	},
	"jest": {
		"setupFilesAfterEnv": [
			"@testing-library/jest-dom/extend-expect"
		],
		"transform": {
			"^.+\\.ts$": "ts-jest",
			"^.+\\.svelte$": [
				"svelte-jester",
				{
					"preprocess": true
				}
			]
		},
		"moduleFileExtensions": [
			"ts",
			"js",
			"svelte"
		]
	},
	"type": "module"
}
